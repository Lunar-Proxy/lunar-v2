---
import '@/main.css';
import SideBar from '../layouts/side.astro';
import Tabs from '../components/tb.astro';
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="generator" content={Astro.generator} />
    <title>Google Docs</title>
    <link rel="icon" type="image/x-icon" href="/favicon.ico" />
    <link rel="prefetch" href="/assets/bundled/scram/worker.js" />
    <link rel="prefetch" href="/assets/bundled/scram/shared.js" />
    <script src="../utils/script.ts"></script>
  </head>
  <body class="h-screen text-white bg-gradient-to-b from-[#09090f] to-[#1a1a2e] overflow-hidden">
     <Tabs />
    <SideBar />
        <main class="flex flex-col items-center justify-center h-full p-6 bg-gray-900 rounded-lg">
          <h1
            class="font-linux text-8xl font-bold text-transparent bg-clip-text bg-gradient-to-r from-blue-300 via-white to-gray-400 drop-shadow-[0_0_15px_rgba(173,216,230,0.8)]"
          >
            Lunar v1
          </h1>
          <br />
          </main>
    </div>
    <script src="/assets/bundled/scram/controller.js" is:inline></script>
    <script src="/bm/index.js" defer is:inline></script>
    <script src="../utils/main.ts"></script>
  </body>
</html>
